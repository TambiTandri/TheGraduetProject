<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.projekakhir.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_menuFragment"
            app:destination="@id/menuFragment" />
        <action
            android:id="@+id/action_homeFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/menuFragment"
        android:name="com.example.projekakhir.ui.menu.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/action_menuFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_menuFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_menuFragment_to_persyaratanFragment"
            app:destination="@id/persyaratanFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.projekakhir.ui.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
        <action
            android:id="@+id/action_loginFragment_to_adminFragment"
            app:destination="@id/adminFragment" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_forgotPasswordFragment"
            app:destination="@id/forgotPasswordFragment" />
    </fragment>
    <fragment
        android:id="@+id/persyaratanFragment"
        android:name="com.example.projekakhir.ui.persyaratan.PersyaratanFragment"
        android:label="fragment_persyaratan"
        tools:layout="@layout/fragment_persyaratan" >
        <action
            android:id="@+id/action_persyaratanFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.example.projekakhir.ui.dashboardfragment.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboardFragment_to_menuDashboardFragment"
            app:destination="@id/menuDashboardFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/menuDashboardFragment"
        android:name="com.example.projekakhir.ui.menudashboard.MenuDashboardFragment"
        android:label="fragment_menu_dashboard"
        tools:layout="@layout/fragment_menu_dashboard" >
        <action
            android:id="@+id/action_menuDashboardFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
        <action
            android:id="@+id/action_menuDashboardFragment_to_sklFragment"
            app:destination="@id/sklFragment" />
        <action
            android:id="@+id/action_menuDashboardFragment_to_pengambilanIjazahFragment"
            app:destination="@id/pengambilanIjazahFragment" />
        <action
            android:id="@+id/action_menuDashboardFragment_to_biodataPendaftaranFragment"
            app:destination="@id/biodataPendaftaranFragment" />
        <action
            android:id="@+id/action_menuDashboardFragment_to_berhasilSimpanPendaftaranFragment"
            app:destination="@id/berhasilSimpanPendaftaranFragment" />
        <action
            android:id="@+id/action_menuDashboardFragment_to_berhasilSimpanIjazahFragment"
            app:destination="@id/berhasilSimpanIjazahFragment" />
    </fragment>
    <fragment
        android:id="@+id/sklFragment"
        android:name="com.example.projekakhir.ui.skl.SklFragment"
        android:label="fragment_skl"
        tools:layout="@layout/fragment_skl" >
        <action
            android:id="@+id/action_sklFragment_to_menuDashboardFragment2"
            app:destination="@id/menuDashboardFragment" />
        <action
            android:id="@+id/action_sklFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/pengambilanIjazahFragment"
        android:name="com.example.projekakhir.ui.pengambilan.PengambilanIjazahFragment"
        android:label="fragment_pengambilan_ijazah"
        tools:layout="@layout/fragment_pengambilan_ijazah" >
        <action
            android:id="@+id/action_pengambilanIjazahFragment_to_menuDashboardFragment"
            app:destination="@id/menuDashboardFragment" />
        <action
            android:id="@+id/action_pengambilanIjazahFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_pengambilanIjazahFragment_to_berhasilSimpanIjazahFragment"
            app:destination="@id/berhasilSimpanIjazahFragment" />
    </fragment>
    <fragment
        android:id="@+id/biodataPendaftaranFragment"
        android:name="com.example.projekakhir.ui.biodatapendaftaran.BiodataPendaftaranFragment"
        android:label="fragment_biodata_pendaftaran"
        tools:layout="@layout/fragment_biodata_pendaftaran" >
        <action
            android:id="@+id/action_biodataPendaftaranFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_biodataPendaftaranFragment_to_menuDashboardFragment"
            app:destination="@id/menuDashboardFragment" />
        <action
            android:id="@+id/action_biodataPendaftaranFragment_to_berkasPendaftaranFragment"
            app:destination="@id/berkasPendaftaranFragment" />
        <action
            android:id="@+id/action_biodataPendaftaranFragment_to_berkasPendaftaranFragment2"
            app:destination="@id/berkasPendaftaranFragment" />
        <action
            android:id="@+id/action_biodataPendaftaranFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
        <action
            android:id="@+id/action_biodataPendaftaranFragment_to_berhasilSimpanPendaftaranFragment"
            app:destination="@id/berhasilSimpanPendaftaranFragment" />
    </fragment>
    <fragment
        android:id="@+id/berkasPendaftaranFragment"
        android:name="com.example.projekakhir.ui.berkaspendaftaran.BerkasPendaftaranFragment"
        android:label="fragment_berkas_pendaftaran"
        tools:layout="@layout/fragment_berkas_pendaftaran" >
        <action
            android:id="@+id/action_berkasPendaftaranFragment_to_menuDashboardFragment"
            app:destination="@id/menuDashboardFragment" />
        <action
            android:id="@+id/action_berkasPendaftaranFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/adminFragment"
        android:name="com.example.projekakhir.ui.admin.AdminFragment.AdminFragment"
        android:label="fragment_admin"
        tools:layout="@layout/fragment_admin" >
        <action
            android:id="@+id/action_adminFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.projekakhir.ui.register.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/berhasilSimpanPendaftaranFragment"
        android:name="com.example.projekakhir.ui.berhasilsimpanpendaftaran.BerhasilSimpanPendaftaranFragment"
        android:label="fragment_berhasil_simpan_pendaftaran"
        tools:layout="@layout/fragment_berhasil_simpan_pendaftaran" >
        <action
            android:id="@+id/action_berhasilSimpanPendaftaranFragment_to_menuDashboardFragment"
            app:destination="@id/menuDashboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.example.projekakhir.ui.splash.SplashScreenFragment"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/main_nav"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashScreenFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment"
            app:popUpTo="@id/main_nav"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashScreenFragment_to_adminFragment"
            app:destination="@id/adminFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/berhasilSimpanIjazahFragment"
        android:name="com.example.projekakhir.ui.berhasilsimpanijasah.BerhasilSimpanIjazahFragment"
        android:label="fragment_berhasil_simpan_ijazah"
        tools:layout="@layout/fragment_berhasil_simpan_ijazah" >
        <action
            android:id="@+id/action_berhasilSimpanIjazahFragment_to_menuDashboardFragment"
            app:destination="@id/menuDashboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/forgotPasswordFragment"
        android:name="com.example.projekakhir.ui.forgotpassword.ForgotPasswordFragment"
        android:label="fragment_forgot_password"
        tools:layout="@layout/fragment_forgot_password" >
        <action
            android:id="@+id/action_forgotPasswordFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
</navigation>